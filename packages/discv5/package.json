{
  "name": "discv5-fork",
  "version": "10.0.5-fake",
  "description": "Discovery V5 mega fork",
  "type": "module",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js"
    },
    "./kademlia": {
      "import": "./lib/kademlia/index.js"
    },
    "./keypair": {
      "import": "./lib/keypair/index.js"
    },
    "./libp2p": {
      "import": "./lib/libp2p/index.js"
    },
    "./packet": {
      "import": "./lib/packet/index.js"
    },
    "./util": {
      "import": "./lib/util/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "lib/*",
        "lib/*/index"
      ]
    }
  },
  "types": "./lib/index.d.ts",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "yarn build",
    "lint": "eslint --color --ext .ts src/ test/",
    "test": "yarn test:unit && yarn test:e2e",
    "test:unit": "mocha 'test/unit/**/*.test.ts'",
    "test:e2e": "mocha 'test/e2e/**/*.test.ts'"
  },
  "pre-push": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acolytec3/discv5.git"
  },
  "author": "Acolyte",
  "license": "Apache-2.0",
  "devDependencies": {},
  "dependencies": {
    "@chainsafe/enr": "^4.0.1",
    "@libp2p/crypto": "^5.0.1",
    "@libp2p/interface": "^2.0.1",
    "@multiformats/multiaddr": "^12.1.10",
    "@noble/hashes": "^1.7.0",
    "@noble/secp256k1": "^2.2.2",
    "bigint-buffer": "^1.1.5",
    "debug": "^4.3.1",
    "lru-cache": "^10.1.0",
    "rlp": "^2.2.6",
    "strict-event-emitter-types": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
