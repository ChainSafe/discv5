{
  "all": true,
  "cache": false,
  "extension": [".ts", ".js", ".jsx", ".tsx"],
  "exclude": [
    "**/node_modules/**",
    "**/bench/**",
    "**/coverage/**",
    "**/test/**"
  ],
  "instrument": true,
  "sourceMap": true,
  "check-coverage": true,
  "reporter": [
    "html",
    "lcov",
    "text",
    "text-summary"
  ]
}
